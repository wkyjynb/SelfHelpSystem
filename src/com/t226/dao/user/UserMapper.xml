<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.t226.dao.user.UserMapper">

<insert id="affairAddUser"><!--添加用户-->
INSERT security_user (password,userName,phone,identityId)VALUE
(#{password},#{userName},#{phone},#{identityId})
</insert>

    <select id="userLogin" resultType="User">
        SELECT * FROM security_user where phone=#{phone}
    </select>

</mapper>